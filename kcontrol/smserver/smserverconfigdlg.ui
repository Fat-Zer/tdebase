                <widget class="TQRadioButton">
                    <property name="name">
                        <cstring>rebootRadio</cstring>
                    </property>
                    <property name="text">
                        <string>&amp;Restart computer</string>
                    </property>
                </widget>
            </vbox>
        </widget>
        <widget class="TQButtonGroup">
